{
    "name": "desafio",
    "version": "1.0.0",
    "main": "server.js",
    "type":"module",
    "scripts": {
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "dev": "node --watch server.js"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.1",
        "jsonwebtoken": "^9.0.2"
    },
    "directories": {
        "test": "tests"
    },
    "keywords": [],
    "description": "",
    "devDependencies": {
        "@babel/preset-env": "^7.23.9",
        "@faker-js/faker": "^8.4.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.4"
    },
    "jest": {
        "transform": {
            "^.+\\.js$": "babel-jest"
        },
        "collectCoverageFrom": [
            "**/*.js"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 100,
                "branches": 100,
                "functions": 100,
                "lines": 100
            }
        }
    }
}
